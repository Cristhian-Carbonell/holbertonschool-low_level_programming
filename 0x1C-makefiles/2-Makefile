################################################
CC = gcc
SRC = main.c holberton.c
OBJ = holberton.o main.o
NAME = holberton
#################################################
# create executable file named holberton
all: $(NAME)
main.o: $(SRC)
	$(CC) -c -o $(OBJ) $(SRC)
holberton.o: $(SRC)
	$(CC) -c -o $(OBJ) $(SRC)
holberton: $(OBJ) m.h
	$(CC) $(OBJ) -o holberton
