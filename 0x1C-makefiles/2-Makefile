################################################
CC = gcc
SRC = main.c holberton.c
OBJ = holberton.o main.o
NAME = holberton
#################################################
# create executable file named holberton
all: $(NAME)
main.o: main.c
	$(CC) -c -o main.o main.c
holberton.o: holberton.c
	$(CC) -c -o holberton.o holberton.c
$(NAME): $(OBJ) m.h
	$(CC) $(OBJ) -o $(NAME)
